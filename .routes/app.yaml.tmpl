---
# Code generated by .routes/main.go; DO NOT EDIT.

runtime: go121
service: default

automatic_scaling:
  max_instances: 1
  max_idle_instances: 0

handlers:
{{- range . }}
{{- if .Directory }}
  - url: {{ .Path }}
    static_dir: .build/{{ .Directory }}
    secure: always
{{- else }}
  - url: {{ .Path }}
    static_files: .build/{{ .File }}
    upload: .build/{{ .File }}
    secure: always
    http_headers:
      Content-Type: {{ .ContentType }}
{{- end }}
{{ end }}
  - url: /.*
    script: auto
    secure: always
